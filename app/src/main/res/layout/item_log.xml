<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="16dp">

    <!-- Log Message -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log Message"
        android:paddingBottom="14dp"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:textSize="24sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/logTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log Message"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Timestamp -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Timestamp:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/timestampTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Timestamp"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- New Owner -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Owner:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/newOwnerTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Owner"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Old Owner -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Old Owner:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/oldOwnerTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Old Owner"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Deal Amount -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deal Amount:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/dealAmountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deal Amount"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Property Size -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Size:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/propertySizeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Size"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Dealer Agent -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dealer Agent:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/dealerAgentTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dealer Agent"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Commission Earned -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Commission Earned:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/commissionEarnedTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Commission Earned"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Property Type -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Type:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/propertyTypeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Type"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Property Address -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Address:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/propertyAddressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Address"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Image URL -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Image URL:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/imageUrlTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Image URL"
        android:autoLink="web"
        android:textSize="18sp"
        android:textColor="@android:color/white" />

    <!-- Separator Line -->
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp" />

</LinearLayout>
